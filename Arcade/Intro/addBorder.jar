String[] addBorder(String[] picture) {
    int len= picture[0].length();
    String[] output=new String[2+picture.length];
    output[0]=new String(new char[len+2]).replace("\0", "*");
    for (int i=1; i<picture.length+1; i++){
        output[i]="*"+picture[i-1]+"*";
    }
    output[picture.length+1]=new String(new char[len+2]).replace("\0", "*");
    System.out.println(output[0]);
    return output;
}
