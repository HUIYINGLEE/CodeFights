int[] sortByHeight(int[] a) {
    for (int i=0; i<a.length; i++){
        for(int j=i+1; j<a.length; j++){
            if(a[i]!=-1 && a[j]!=-1 && a[i]>a[j]){
                int larger;
                larger=a[i];
                a[i]=a[j];
                a[j]=larger;
            }
        }
    }
    return a;
}
