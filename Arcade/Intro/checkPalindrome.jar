boolean checkPalindrome(String inputString) {
    int stringLen= inputString.length();
    char[] chars=inputString.toCharArray();
    int i1 = 0;
    int i2 = stringLen - 1;
    while (i2 > i1) {
        if (chars[i1] != chars[i2]) {
            return false;
        }
        ++i1;
        --i2;
    }
    return true;
}


